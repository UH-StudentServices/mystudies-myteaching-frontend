{
  "name": "opintoni",
  "version": "0.0.0",
  "description": "Description for opintoni",
  "private": true,
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "bower": "^1.7.9",
    "concurrently": "^2.1.0",
    "connect-modrewrite": "^0.8.1",
    "eslint": "^1.10.3",
    "eslint-config-defaults": "^9.0.0",
    "event-stream": "3.2.1",
    "grunt": "^1.0.1",
    "grunt-browser-sync": "^2.2.0",
    "grunt-build-control": "^0.7.0",
    "grunt-concurrent": "^2.3.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-compass": "^1.1.1",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^1.0.1",
    "grunt-contrib-htmlmin": "^1.4.0",
    "grunt-contrib-imagemin": "^1.0.1",
    "grunt-contrib-uglify": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-filerev": "^2.3.1",
    "grunt-html2js": "^0.3.6",
    "grunt-karma": "^2.0.0",
    "grunt-modernizr": "^1.0.2",
    "grunt-ng-annotate": "^2.0.2",
    "grunt-ng-constant": "^2.0.1",
    "grunt-postcss": "^0.8.0",
    "grunt-protractor-runner": "^3.2.0",
    "grunt-sass": "^1.2.0",
    "grunt-svgmin": "^3.2.0",
    "grunt-text-replace": "^0.4.0",
    "grunt-usemin": "^3.1.1",
    "grunt-wiredep": "^3.0.1",
    "gruntify-eslint": "^1.3.0",
    "html-inject-script": "^1.1.0",
    "http-proxy": "^1.11.1",
    "karma": "^1.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-html2js-preprocessor": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-requirejs": "^1.0.0",
    "karma-script-launcher": "^1.0.0",
    "load-grunt-tasks": "^3.5.0",
    "lodash": "2.4.1",
    "node-uuid": "^1.4.3",
    "phantomjs-prebuilt": "^2.1.5",
    "protractor-screenshot-reporter": "0.0.5",
    "requirejs": "~2.1",
    "time-grunt": "1.0.0",
    "zeparser": "0.0.7"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "bower:install": "bower install",
    "build": "grunt build --gruntfile main/Gruntfile.js && grunt build --gruntfile portfolio/Gruntfile.js",
    "dev": "concurrently 'grunt serve --gruntfile main/Gruntfile.js' 'grunt serve --gruntfile portfolio/Gruntfile.js'",
    "distqa": "grunt buildMinified --gruntfile main/Gruntfile.js && grunt buildMinified --gruntfile portfolio/Gruntfile.js",
    "distprod": "grunt buildMinified --gruntfile main/Gruntfile.js && grunt buildMinified --gruntfile portfolio/Gruntfile.js && npm run inject:newrelic",
    "lint": "eslint common/** main/** portfolio/**",
    "postinstall": "concurrently 'npm run webdriver:update' 'npm run bower:install'",
    "predist": "npm rebuild node-sass",
    "test": "concurrently 'grunt test --gruntfile main/Gruntfile.js' 'grunt test --gruntfile portfolio/Gruntfile.js'",
    "webdriver:update": "node_modules/grunt-protractor-runner/node_modules/protractor/bin/webdriver-manager update",
    "inject:newrelic": "mv main/dist/index.html main/dist/index.html.tmp && cat main/dist/index.html.tmp | htmlinjectscript \"assets/scripts/newrelic.js\" > main/dist/index.html && rm main/dist/index.html.tmp"
  }
}
