    "http-proxy": "^1.17.0",
    "jasmine-core": "^2.8.0",
    "jsonlint": "^1.6.3",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "^2.0.0",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-requirejs": "^1.1.0",
    "karma-script-launcher": "^1.0.0",
    "load-grunt-tasks": "^4.0.0",
    "node-uuid": "^1.4.3",
    "phantomjs-prebuilt": "^2.1.14",
    "testcafe": "^0.20.4",
    "time-grunt": "1.4.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "bower:install": "bower install",
    "gitinfo": "node scripts/git-info.js",
    "build": "grunt build --gruntfile main/Gruntfile.js && grunt build --gruntfile profile/Gruntfile.js",
    "dev": "concurrently 'grunt serve --gruntfile main/Gruntfile.js' 'grunt serve --gruntfile profile/Gruntfile.js'",
    "dist": "grunt buildMinified --gruntfile main/Gruntfile.js && grunt buildMinified --gruntfile profile/Gruntfile.js && npm run gitinfo",
    "lint": "eslint 'common/' 'main/' 'profile/'",
    "lintfix": "eslint 'common/' 'main/' 'profile/' --fix",
    "postinstall": "npm run bower:install",
    "predist": "npm rebuild node-sass",
    "prepush": "npm test",
    "pretest": "npm run lint",
    "test": "concurrently 'grunt test --gruntfile main/Gruntfile.js' 'grunt test --gruntfile profile/Gruntfile.js' 'node scripts/i18nTest.js'",
    "test:e2e": "cross-env NODE_ENV=local testcafe 'chrome -incognito' -S -s screenshots common/test/e2e",
    "test:e2e:ci": "cross-env NODE_ENV=ci testcafe 'chromium:headless' -S -s screenshots common/test/e2e"
  },
  "dependencies": {
    "git-rev-sync": "^1.12.0",
    "husky": "^0.14.3"
  }
}
