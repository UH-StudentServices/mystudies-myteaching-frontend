{
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "extends": "plugin:testcafe/recommended",
  "plugins": [
    "testcafe",
    "import"
  ],
  "rules": {
    "no-unused-expressions": [2, {
      "allowTaggedTemplates": true
    }]
  }
}
