{
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "plugins": [
    "testcafe",
    "import"
  ],
  "extends": "plugin:testcafe/recommended",
  "rules": {
    "no-unused-expressions": [2, {
      "allowTaggedTemplates": true
    }],
    "new-cap": [2, {"capIsNewExceptions": ["Selector"]}]
  }
}
