{
  "extends": "airbnb-base/legacy",
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "_": true,
    "$": false,
    "afterEach": false,
    "angular": false,
    "beforeEach": false,
    "browser": false,
    "by": false,
    "Clipboard": false,
    "describe": false,
    "element": false,
    "expect": false,
    "google": false,
    "Hammer": false,
    "inject": false,
    "it": false,
    "jasmine": false,
    "Modernizr": false,
    "moment": false,
    "RSSParser": true,
    "sprintf": false,
    "twttr": true,
    "CKEDITOR": true
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "header/header": [2, "header.txt"],
    "func-names": 0,
    "no-param-reassign": ["error",
      { "props": false }
    ],
    "vars-on-top": 0,
    "operator-linebreak": ["error", "before", { "overrides": { "=": "ignore" } }]
  },
  "plugins": [
    "header"
  ]
}
